    "@angular/platform-server": "4.3.5",
    "@angular/router": "4.3.5",
    "@angularclass/conventions-loader": "1.0.13",
    "@angularclass/hmr": "2.1.3",
    "@angularclass/hmr-loader": "~3.0.2",
    "@covalent/core": "^1.0.0-beta.6",
