    "@angular/animations": "4.3.5",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "4.3.5",
    "@angular/compiler": "4.3.5",
    "@angular/core": "4.3.5",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.3.5",
