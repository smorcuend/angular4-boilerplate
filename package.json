{
  "name": "angular2-boilerplate",
  "version": "0.0.0",
  "description": "Angular2 Boilerplate by seedtag",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "bootstrap"
  ],
  "author": "Dev Team <dev@seedtag.com>",
  "homepage": "https://github.com/seedtag/angular2-boilerplate",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "yarn run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "yarn run build:dev",
    "ci": "yarn run lint && yarn test && yarn run e2e",
    "clean:dist": "yarn run rimraf -- dist",
    "clean:install": "yarn set progress=false && yarn install",
    "clean:start": "yarn start",
    "clean": "yarn cache clean && yarn run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docs": "yarn run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "yarn run e2e -- --elementExplorer",
    "e2e": "yarn run protractor",
    "lint": "yarn run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "yarn run clean:dist",
    "prebuild:prod": "yarn run clean:dist",
    "preclean:install": "yarn run clean",
    "preclean:start": "yarn run clean",
    "pree2e": "yarn run webdriver:update -- --standalone",
    "preversion": "yarn test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --public '0.0.0.0:8989' --progress --profile --watch --content-base src/ --no-info",
    "server:prod": "webpack-dev-server --config config/webpack.prod.js --public '0.0.0.0:8989' --progress --profile --watch --content-base dist/ --no-info",
    "server": "yarn run server:dev",
    "start:hmr": "yarn run server:dev:hmr",
    "start": "yarn run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "yarn run build",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "yarn run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "dev": "yarn run watch",
    "prod": "yarn run build:prod"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@angular/router": "3.4.1",
    "@angularclass/conventions-loader": "1.0.13",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "animate.css": "^3.5.2",
    "bootstrap": "4.0.0-alpha.5",
    "bootstrap-datepicker": "^1.6.4",
    "bourbon": "^4.2.7",
    "core-js": "^2.4.1",
    "jquery": "^3.1.0",
    "rxjs": "5.0.2",
    "tether": "1.4.0",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.40",
    "@types/node": "6.0.56",
    "@types/selenium-webdriver": "2.53.38",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "^2.0.28",
    "@types/webpack": "2.1.0",
    "angular2-template-loader": "0.6.0",
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "6.6.1",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "bootstrap-loader": "2.0.0-beta.18",
    "codelyzer": "2.0.0-beta.4",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.26.0",
    "imports-loader": "0.7.0",
    "istanbul-instrumenter-loader": "1.2.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.1",
    "node-sass": "4.1.1",
    "parse5": "3.0.1",
    "postcss": "5.2.8",
    "postcss-loader": "1.2.1",
    "protractor": "4.0.14",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "^2.5.2",
    "sass-loader": "4.1.1",
    "script-ext-html-webpack-plugin": "1.4.0",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "2.0.0",
    "tslint": "~4.2.0",
    "typedoc": "0.5.3",
    "typescript": "2.1.4",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "github.com/seedtag/angular2-boilerplate.git"
  },
  "bugs": {
    "url": "github.com/seedtag/angular2-boilerplate/issues"
  },
  "engines": {
    "node": "^6.9.0",
    "yarn": "^0.18.0"
  }
}
