{
  "name": "ngx-boilerplate",
  "version": "0.2.0",
  "description": "Angular Boilerplate",
  "keywords": [
    "angular",
    "webpack",
    "typescript",
    "bootstrap"
  ],
  "author": "Sergio Morcuende <smorcuend@gmail.com>",
  "homepage": "https://github.com/smorcuend/ngx-boilerplate",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "yarn run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "yarn run build:dev",
    "ci": "yarn run lint && yarn test && yarn run e2e",
    "clean:dist": "yarn run rimraf -- dist",
    "clean:install": "yarn set progress=false && yarn install",
    "clean:start": "yarn start",
    "clean": "yarn cache clean && yarn run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docs": "yarn run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "yarn run e2e -- --elementExplorer",
    "e2e": "yarn run protractor",
    "lint": "yarn run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "yarn run clean:dist",
    "prebuild:prod": "yarn run clean:dist",
    "preclean:install": "yarn run clean",
    "preclean:start": "yarn run clean",
    "pree2e": "yarn run webdriver:update -- --standalone",
    "preversion": "yarn test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --public '0.0.0.0:8989' --progress --profile --watch --content-base src/ --no-info",
    "server:prod": "webpack-dev-server --config config/webpack.prod.js --public '0.0.0.0:8989' --progress --profile --watch --content-base dist/ --no-info",
    "server": "yarn run server:dev",
    "start:hmr": "yarn run server:dev:hmr",
    "start": "yarn run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "yarn run build",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "yarn run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "dev": "yarn run watch",
    "prod": "yarn run build:prod"
  },
